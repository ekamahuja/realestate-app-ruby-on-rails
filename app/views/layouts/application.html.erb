    <!-- Custom StyleSheet -->
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <!-- Style Sheet -->
    <link rel="stylesheet" href="https://creativelayers.net/themes/houzing-html/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://creativelayers.net/themes/houzing-html/css/style.css">
    <!-- Responsive stylesheet -->
