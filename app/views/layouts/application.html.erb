    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Basic Page Info -->
    <title>Realestate Application</title>
    <!-- CSRF -->
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <!-- Bootstrap CSS -->
